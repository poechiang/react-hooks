    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "json": "^11.0.0",
    "storybook": "^8.0.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-static-copy": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || 18.x",
    "react-dom": "^16.8.0 || 18.x",
    "styled-components": "^6.1.8"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/r-components.umd.js",
  "module": "./dist/r-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/r-components.es.js",
      "require": "./dist/r-components.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./themes/*": {
      "import": "./dist/styles/index.less"
    }
